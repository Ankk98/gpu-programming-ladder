{
  "system": {
    "name": "GPU Programming Ladder URL Validator",
    "version": "1.0.0",
    "description": "AI-powered URL validation system for GPU programming educational content"
  },
  "lm_studio": {
    "url": "http://localhost:1234",
    "model": "openai/gpt-oss-20b",
    "timeout": 120,
    "max_tokens": 500,
    "temperature": 0.1
  },
  "agents": {
    "num_consumer_agents": 2,
    "max_concurrent_requests_per_agent": 2,
    "request_timeout": 120,
    "rate_limit_delay": 1.0
  },
  "files": {
    "data_js": "../data.js",
    "urls_to_validate": "urls_to_validate.json",
    "validation_tasks": "validation_tasks.json",
    "validation_results": "validation_results.json"
  },
  "processing": {
    "update_data_js": true,
    "force_revalidation": false,
    "batch_size": 10,
    "max_retries": 3
  },
  "validation_rules": {
    "check_existence": true,
    "check_content_appropriateness": true,
    "use_llm_content_analysis": true,
    "llm_analysis_fallback": true,
    "require_specific_content": [
      "exercise",
      "video"
    ],
    "allow_listing_pages": [
      "article",
      "paper"
    ],
    "github_repo_depth_check": true
  },
  "external_apis": {
    "firecrawl": {
      "enabled": false,
      "api_key": "your-firecrawl-api-key-here",
      "base_url": "https://api.firecrawl.dev"
    },
    "brave_search": {
      "enabled": false,
      "api_key": "your-brave-api-key-here",
      "base_url": "https://api.search.brave.com"
    }
  },
  "mcp_tools": {
    "enabled": false,
    "servers": {
      "firecrawl": "http://localhost:3000",
      "context7": "http://localhost:3001",
      "brave_search": "http://localhost:3002"
    }
  }
}